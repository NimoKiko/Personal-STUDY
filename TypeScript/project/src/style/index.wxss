* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
body {
  font: bold 20px "Courier";
}
#main {
  width: 360px;
  height: 420px;
  margin: 50px auto;
  background-color: #b7d4a8;
  border: 10px solid black;
  border-radius: 30px;
}
#main #stage {
  width: 304px;
  height: 304px;
  border: 2px solid black;
  margin: 20px auto;
  position: relative;
}
#main #stage #snake div {
  width: 10px;
  height: 10px;
  background-color: black;
  border: 1px solid #b7d4a8;
  position: absolute;
}
#main #stage #food {
  width: 10px;
  height: 10px;
  background-color: black;
  border: 1px solid #b7d4a8;
  border-radius: 100px;
  position: absolute;
  left: 40px;
  top: 100px;
}
#main #score-panel {
  width: 304px;
  margin: 0px auto;
  display: flex;
  justify-content: space-between;
}
